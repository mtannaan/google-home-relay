{
  "name": "google-home-relay",
  "version": "0.1.0",
  "description": "Smart Home service fulfillment that simply relays requests to another server",
  "main": "build/index.js",
  "scripts": {
    "start": "npm run compile && node build/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint",
    "preinstall": "npx npm-force-resolutions",
    "jwt-to-env": "ls -f jwt/* | head -1 | xargs node -e \"console.log('TOKEN_CREATOR_JWT='+ require('fs').readFileSync(process.argv[1]).toString('hex'))\" && echo && echo 'Paste string above to .env and heroku config:set!'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mtannaan/google-home-relay.git"
  },
  "keywords": [
    "google",
    "actions",
    "smart",
    "home",
    "google",
    "assistant"
  ],
  "author": "mtannaan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mtannaan/google-home-relay/issues"
  },
  "homepage": "https://github.com/mtannaan/google-home-relay#readme",
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/node": "^14.11.2",
    "gts": "^3.0.3",
    "npm-force-resolutions": "0.0.3",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "actions-on-google": "^2.13.0",
    "axios": "^0.21.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1"
  },
  "resolutions": {
    "node-forge": "0.10.0",
    "axios": "0.21.1"
  }
}
